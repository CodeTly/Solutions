def main(l,target):
    ln=[]
    for i in range(0,len(l)):
        for j in range(0, len(l)):
            if l[i]+l[j]==target and i!=j:
                ln.append(i)
                ln.append(j)
                print(ln)
                return
main([2, 7, 11, 15],9)#[0,1]
main([3, 2, 4],6)#[1,2]
main([3,3],6)#[0,1]
